wget https://repo1.maven.org/maven2/org/apache/poi/poi/5.2.3/poi-5.2.3.jar
wget https://repo1.maven.org/maven2/org/apache/poi/poi-ooxml/5.2.3/poi-ooxml-5.2.3.jar
wget https://repo1.maven.org/maven2/org/apache/poi/poi-ooxml-schemas/4.1.2/poi-ooxml-schemas-4.1.2.jar
wget https://repo1.maven.org/maven2/org/apache/commons/commons-collections4/4.4/commons-collections4-4.4.jar
wget https://repo1.maven.org/maven2/org/apache/commons/commons-compress/1.21/commons-compress-1.21.jar

  # 컴파일
javac -cp ".:poi-5.2.3.jar:poi-ooxml-5.2.3.jar:poi-ooxml-schemas-4.1.2.jar:commons-collections4-4.4.jar:commons-compress-1.21.jar" QuickSortExample.java

# 실행
java -cp ".:poi-5.2.3.jar:poi-ooxml-5.2.3.jar:poi-ooxml-schemas-4.1.2.jar:commons-collections4-4.4.jar:commons-compress-1.21.jar" QuickSortExample
